{
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 */3 * *"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "sinamokevin.vercel.app"
        }
      ],
      "destination": "https://www.sinamokevin.my.id/$1",
      "permanent": true
    }
  ]
}
